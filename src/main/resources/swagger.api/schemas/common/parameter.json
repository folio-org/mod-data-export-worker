{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "description": "List of key/value parameters of an error",
  "Parameter": {
    "type": "object",
    "properties": {
      "key": {
        "type": "string",
        "minLength": 1
      },
      "value": {
        "type": "string"
      }
    },
    "additionalProperties": false,
    "required": [
      "key"
    ]
  }
}
